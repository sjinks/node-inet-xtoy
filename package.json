{
  "name": "inet_xtoy",
  "version": "1.2.5",
  "description": "inet_ntop() and inet_pton() bindings for Node.js",
  "main": "index.js",
  "types": "./index.d.ts",
  "scripts": {
    "install": "prebuild-install --runtime napi --tag-prefix '' || node-gyp rebuild",
    "build": "node-gyp rebuild",
    "build:dev": "node-gyp rebuild --debug",
    "test": "mocha tests"
  },
  "keywords": [
    "ip",
    "inet_ntop",
    "inet_pton",
    "ipv4",
    "ipv6"
  ],
  "author": "Volodymyr Kolesnykov <volodymyr@wildwolf.name> (https://wildwolf.name/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjinks/node-inet-xtoy.git"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "main.cpp",
    "binding.gyp"
  ],
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "*",
    "prebuild-install": "^7.0.0"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "node-gyp": "^9.0.0",
    "prebuild": "^12.0.0",
    "should": "^13.2.3"
  },
  "overrides": {
    "prebuild": {
      "node-gyp": "$node-gyp"
    },
    "node-gyp": {
      "semver": "^7.5.2"
    }
  },
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/sjinks/node-inet-xtoy/issues"
  },
  "homepage": "https://github.com/sjinks/node-inet-xtoy#readme",
  "publishConfig": {
    "access": "public"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  }
}
